""" e-Judge """
def main():
    """ Password Checking """
    password = input()
    print('Enter your password...')
    for i in range(4):
        guess = input()
        if guess == password:
            print('Welcome, Mr.Natchanon.')
            break
        else:
            if i == 3 and (guess != password):
                print('Incorrect, Try again in 30 seconds.')
            else:
                print('Incorrect, Please Enter password again.')
main()
